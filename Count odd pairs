
public class Solution {

    public static void main(String[] args) {
        int arr[] = {0, 5, 6, 1, 3};
        int n = arr.length;
        for(int i = 0 ; i < n ; i++){
            for(int j = 0 ; j < n ; j++){
                if((arr[i] + arr[j]) % 2 != 0){
                    System.out.println(arr[i] + " " + arr[j]);
                }
            }
        }
    }
}
